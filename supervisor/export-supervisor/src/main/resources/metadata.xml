
<ipojo>
	<component name="rose.export-supervisor"
			classname="org.ow2.chameleon.rose.supervisor.ExportSupervisor">

		<properties propagation="true">
			<property name="export.filter" method="setExportFilter" mandatory="true"/>
		</properties>

		<requires field="logger" optional="true" />

		<callback transition="validate" method="start" />
		<callback transition="invalidate" method="stop" />

		<requires optional="false" id="exporter-service">
			<callback type="bind" method="bindExporterService"/>
		</requires>
	</component>
</ipojo>

